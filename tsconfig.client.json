{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "module": "esnext",
    "target": "esnext",
    "importHelpers": true,
    "noUnusedParameters": true,
    "noUnusedLocals": true,
    "strict": true,
    "lib": [
      "dom",
      "es2016",
      "es2017",
      "es2018"
    ],
  },
  "include": [
    "./src/main/resources/assets/**/*.ts",
    "./src/main/resources/assets/**/*.tsx",
  ]
}
